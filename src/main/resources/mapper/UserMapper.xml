<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wild.mapper.WUserMapper">
	<insert id="register" parameterType="WUser">
		insert into wuser
		(WID,WName,WUserNum,WPassWord,WDate,WStatus,WSuperManager)
		values(#{WID},#{WName},#{WUserNum},#{WPassWord},#{WDate},#{WStatus},#{WSuperManager})
	</insert>
	
	<update id="perfectDatum" parameterType="WUser">
		update wuser set wsex=#{WSex},wage=#{WAge} where wid=#{WID}
	</update>
</mapper>